export enum WebsocketStatus {
    CONNECTING = 'CONNECTING...',
    ONLINE = 'ONLINE',
    OFFLINE = 'OFFLINE',
}

export type TFeedIngredient {
    _id: string;
    createdAt: string;
    updatedAt: string;
    status: 'created' | 'pending' | 'done';
    number: number;
    name: string;
    ingredients: string[];
}

export type TDataFeed {
    success: boolean;
    total: number;
    totalToday: number;
    orders: TFeedIngredient[];
}

export type TInitialStateFeed {
    status: WebsocketStatus;
    connectionError: null | string;
    data: IDataFeed | null;
}